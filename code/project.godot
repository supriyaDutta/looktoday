config_version=5

[application]

config/name="gmdemo"
run/main_scene="res://scene/entry_scene.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"
run/size/window_height_override=640
run/size/window_width_override=896

[autoload]

SceneTransition="*res://scene/scene_transition.tscn"
BackgroundMusic="*res://scene/background_music.tscn"

[display]

window/size/viewport_width=224
window/size/viewport_height=160
window/size/window_width_override=896
window/size/window_height_override=640
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

